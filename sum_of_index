function printSumOfIndexes(N, M, arr) {
    for (let i = 0; i < N; i++) {
        let result = '';
        for (let j = 0; j < M; j++) {
            result += (i + j) + ' ';
        }
        console.log(result.trim());
    }
}
const N = 3;
const M = 2;
const arr = [
    [1, 2],
    [3, 4],
    [5, 6]
];
printSumOfIndexes(N, M, arr);
