let studentScores = {
  John: 85,
  Emma: 92,
  Sam: 67,
  Bob: 45
};
for (let student in studentScores) {
  let score = studentScores[student];
  let grade;
  (score >= 90)? grade = "A":
  (score >= 80)? grade = "B":
  (score >= 70)? grade = "C":
  (score >= 60)? grade = "D":
    grade = "F";
  console.log(`${student} - ${grade}`);
}
